---
import Furnitures from '@/layouts/Furnitures.astro';
import Bg from '../../src/images/furnitureBg.png'

const title = "Stoły";
const description = "Stoły";
const background = Bg;
const styles = ["Modern", "Loft", "Industrialny", "Skandynawski"];
const items = [{
    itemStyle: "Loft",
    itemPhotoUrl: "public/images/table1.png"
},{
    itemStyle: "Modern",
    itemPhotoUrl: "public/images/table1.png"
},{
    itemStyle: "Skandynawski",
    itemPhotoUrl: "public/images/table1.png"
},{
    itemStyle: "Loft",
    itemPhotoUrl: "public/images/table1.png"
},{
    itemStyle: "Industrialny",
    itemPhotoUrl: "public/images/table1.png"
}

];
---


<Furnitures title={title} description={description} background={background} styles={styles} items={items}>