---
const {title,stage} = Astro.props;
---

<div class="flex border-b-2 border-gray-200 pb-3 space-x-5 w-full">
    <h1>{stage}</h1>
    <h2>{title}</h2>
</div>

<style>
    h1{
        color: #B99470;
        
        font-size: 1.5rem;
    }
    
    h2{
        font-size: 1.5rem;
        color: rgba(0, 0, 0, 0.558);
    }
   
</style>